
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Movies</h1>

<table class="table">
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Name</th>
    </tr>

    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Id</td>
            <td>@movie.Name</td>
        </tr>
    }

</table>

